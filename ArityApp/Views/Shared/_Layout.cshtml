            $(".dt-button.buttons-pdf.buttons-html5").addClass("btn btn-default notika-btn-default waves-effect");
            $(".dt-button.buttons-pdf.buttons-html5").removeClass("dt-button buttons-pdf buttons-html5");

            $(".dt-button.buttons-excel.buttons-html5").addClass("btn btn-default notika-btn-default waves-effect");
            $(".dt-button.buttons-excel.buttons-html5").removeClass("dt-button buttons-excel buttons-html5");

             $("#logout").click(function () {
               $.ajax({
                url: '@Url.Action("Logout", "account")',
                dataType: "json",
                type: "GET",
                contentType: 'application/json; charset=utf-8',
                   success: function (response) {
                       location.href = "/account/login";
                },
                error: function (error) {
                    Notification('', 'Something went wrong, use different username', 'danger');
                }
            });
        });

        $("body").on("click","#btnCancel",function () {
            $(".close").trigger("click");
        });
        });

        function validateEmail(email) {
            var re = /^(([^<>()\[\]\\.,;:\s@@"]+(\.[^<>()\[\]\\.,;:\s@@"]+)*)|(".+"))@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(String(email).toLowerCase());
        }
    </script>
