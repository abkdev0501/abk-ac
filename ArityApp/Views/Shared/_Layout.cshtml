@using Arity.Data.Helpers

<!doctype html>
<html class="no-js" lang="">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Dashboard | CA</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- favicon
        ============================================ -->
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.ico">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/bootstrap.min.css">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/font-awesome.min.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/owl.carousel.css">
    <link rel="stylesheet" href="/Content/css/owl.theme.css">
    <link rel="stylesheet" href="/Content/css/owl.transitions.css">
    <!-- meanmenu CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/meanmenu/meanmenu.min.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/animate.css">
    <link href="/Content/css/notification/notification.css" rel="stylesheet" />
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/normalize.css">
    <link href="/Content/css/datapicker/datepicker3.css" rel="stylesheet" />
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- jvectormap CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/jvectormap/jquery-jvectormap-2.0.3.css">
    <!-- notika icon CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/notika-custom-icon.css">
    <!-- bootstrap select CSS
    ============================================ -->
    <link rel="stylesheet" href="/Content/css/bootstrap-select/bootstrap-select.css">
    <!-- wave CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/wave/waves.min.css">
    <link href="~/Content/css/wave/button.css" rel="stylesheet" />
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/main.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/style.css">
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="/Content/css/responsive.css">
    <!-- modernizr JS
        ============================================ -->
    <script src="/Content/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    <div id="loading">
        <img id="loading-image" src="~/Content/img/loading.svg" alt="Loading...">
    </div>
    <!-- Start Header Top Area -->
    <div class="header-top-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="logo-area">
                        @*<a href="@Url.Action("index","home")"><img src="~/Content/img/RMEI.jpg" style="width:65%;" alt="" /></a>*@
                    </div>
                </div>

                <ul class="nav navbar-nav notika-top-nav">
                    <li class="nav-item">
                        <a href="/Home/Payment" style="font-size:15px;" class="nav-link"><span>Payment</span></a>
                    </li>
                    @if (Convert.ToInt32(SessionHelper.UserTypeId) != (int)Arity.Service.Core.UserType.User)
                    {
                        <li class="nav-item">
                            <a href="/user" style="font-size:15px;" class="nav-link"><span>User</span></a>
                        </li>
                        <li class="nav-item">
                            <a href="/particular" style="font-size:15px;" class="nav-link"><span>Particular</span></a>
                        </li>
                    }
                    <li class="nav-item">
                        <a href="/Home/Index" style="font-size:15px;" class="nav-link" id="btnHome"><span>Invoice</span></a>
                    </li>
                    <li class="nav-item">
                        <a href="#"  data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle" id="btnProfile"><span><i class="notika-icon notika-support"></i></span></a>
                    </li>
                    <li><a href="#" id="logout"  class="nav-link" ><span><i class="fa fa-sign-in" aria-hidden="true"></i></span></a></li>
                </ul>

                <div>
                </div>

            </div>
        </div>
    </div>

    @RenderBody()


    <!-- Start Footer area-->
    <div class="footer-copyright-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="footer-copy-right">
                        <p>
                            @System.Configuration.ConfigurationManager.AppSettings["FooterMessage"]
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Footer area-->
    <!-- jquery
        ============================================ -->
    <script src="/Content/js/vendor/jquery-1.12.4.min.js"></script>
    <!-- bootstrap JS
        ============================================ -->
    <script src="/Content/js/bootstrap.min.js"></script>
    <!-- wow JS
        ============================================ -->
    <script src="/Content/js/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="/Content/js/jquery-price-slider.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="/Content/js/owl.carousel.min.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="/Content/js/jquery.scrollUp.min.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="/Content/js/meanmenu/jquery.meanmenu.js"></script>
    <!-- counterup JS
        ============================================ -->
    <script src="/Content/js/counterup/jquery.counterup.min.js"></script>
    <script src="/Content/js/counterup/waypoints.min.js"></script>
    <script src="/Content/js/counterup/counterup-active.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="/Content/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- jvectormap JS
        ============================================ -->
    <script src="/Content/js/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="/Content/js/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="/Content/js/jvectormap/jvectormap-active.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="/Content/js/sparkline/sparkline-active.js"></script>
    <script src="/Content/js/sparkline/jquery.sparkline.min.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="/Content/js/flot/jquery.flot.js"></script>
    <script src="/Content/js/flot/jquery.flot.resize.js"></script>
    <script src="/Content/js/flot/curvedLines.js"></script>
    <script src="/Content/js/flot/flot-active.js"></script>
    <!-- knob JS
        ============================================ -->
    <script src="/Content/js/knob/jquery.knob.js"></script>
    <script src="/Content/js/knob/jquery.appear.js"></script>
    <script src="/Content/js/knob/knob-active.js"></script>
    <!--  wave JS
        ============================================ -->
    <script src="/Content/js/wave/waves.min.js"></script>
    <script src="/Content/js/wave/wave-active.js"></script>
    <script src="/Content/js/notification/bootstrap-growl.min.js"></script>
    <script src="/Content/js/notification/notification-active.js"></script>
    <!--  todo JS
        ============================================ -->
    <script src="/Content/js/todo/jquery.todo.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="/Content/js/plugins.js"></script>
    <script src="/Scripts/datapicker/bootstrap-datepicker.js"></script>
    <script src="/Scripts/datapicker/datepicker-active.js"></script>

    <!-- bootstrap select JS
      ============================================ -->
    <script src="/Content/js/bootstrap-select/bootstrap-select.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="/Content/js/main.js"></script>

    <script src="/Content/js/data-table/jquery.dataTables.min.js"></script>
    <script src="/Content/js/data-table/data-table-act.js"></script>
    @RenderSection("scripts", required: false)
    <script>
        $("#logout").click(function () {
               $.ajax({
                url: '@Url.Action("Logout", "account")',
                dataType: "json",
                type: "GET",
                contentType: 'application/json; charset=utf-8',
                   success: function (response) {
                       location.href = "/account/login";
                },
                error: function (error) {
                    Notification('', 'Something went wrong, use different username', 'danger');
                }
            });
        });
    </script>
</body>
</html>


